<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>

	<!-- General -->
	<category label="30101">
		<setting type="sep" />
		<setting id="perpage" type="enum" label="30005" values="25|50|100|250|500|1000|2000" default="3"/>
		<setting id="popperpage" type="enum" label="30007" values="25|50|100|250|500|1000|2000" default="2"/>
		<setting id="allperpage" type="enum" label="30008" values="25|50|100|250|500|1000|2000" default="4"/>
		<setting id="searchperpage" type="enum" label="30009" values="25|50|100|250|500|1000|2000" default="2"/>
        <setting type="sep" />
		<setting id="viewenable" type ="bool" label="30050" default="true"/>
		<setting id="defaultview" type="enum" lvalues="30051|30052|30053|30054|30055|30056" label="30058" default="3"/>
		<setting type="sep" />
	</category>

	<!-- Hulu+ Login -->
	<category label="30102">
		<setting type="sep" />
		<setting id='enable_plus' type ="bool" label="30006" default="false"/>
		<setting id='enable_login' type ="bool" label="30003" default="false"/>
		<setting type="sep" />
		<setting id="login_name" type="text" label="30001" default=""/>
		<setting id="login_pass" type="text" label="30002" default="" option="hidden"/>
		<setting type="sep" />
	</category>

	<!-- Streaming -->
	<category label="30103">
		<setting type="sep" />
		<setting id='queueremove' type ="bool" label="30046" default="false"/>
        <setting id='enable_captions' type ="bool" label="30026" default="false"/>
